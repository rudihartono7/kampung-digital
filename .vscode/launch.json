    "version": "0.2.0",
    "configurations": [
        {
            "name": "KampDigi.WebApp",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/Persentation/Trisatech.KampDigi.WebApp/bin/Debug/net6.0/Trisatech.KampDigi.WebApp.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/Persentation/Trisatech.KampDigi.WebApp",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/src/Persentation/Trisatech.KampDigi.WebApp/Views"
            }
        },
        {
            "name": "KampDigi.Public",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-public",
            "program": "${workspaceFolder}/src/Persentation/Trisatech.KampDigi.Public/bin/Debug/net6.0/Trisatech.KampDigi.Public.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/Persentation/Trisatech.KampDigi.Public",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/src/Persentation/Trisatech.KampDigi.Public/Views"
            }
        }

    ]
}