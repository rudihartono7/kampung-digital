<script type="text/javascript">
	$(document).ready(function () {
		//Show modal for delete confirmation
		$(document).on("click", "#btn-delete", function () {

			var id = $(this).data('id');
			var Year = $(this).data('year');
			var Nominal = $(this).data('nominal');
			console.log('id : ' + id);
			//ubah value yang id = itemId yang ada di dalam class .modal-body
			$(".modal-body #itemId").val(id);
			$(".modal-body #itemYear").val(Year);
			$(".modal-body #itemNominal").val(Nominal);
			//tampilkan modal/dialog
			$("#confirmDelete").modal('show');
		});

		$(document).on("click", "#btn-edit", function () {

			var id = $(this).data('id');
			var Year = $(this).data('year');
			var Nominal = $(this).data('nominal');
			var Montly = $(this).data('montly');
			var duedate = $(this).data('duedate');
			console.log('id : ' + id);
			//ubah value yang id = itemId yang ada di dalam class .modal-body
			$(".modal-body #itemId").val(id);
			$(".modal-body #itemYear").val(Year)
			$(".modal-body #itemNominal").val(Nominal)
			$(".modal-body #itemMontly").val(Montly)
			$(".modal-body #itemDuedate").val(duedate)
			//tampilkan modal/dialog
			$("#confirmEdit").modal('show');
		});
	});
</script>
